#!/bin/bash
make clean
#
# The standard bootloaders.
make atmega1284_w5100
make atmega1284_w5200
make atmega1284_w5500

make atmega2560_w5100
make atmega2560_w5200
make atmega2560_w5500

make atmega328_ethernet

make atmega328_w5100
make atmega328_w5200
make atmega328_w5500

make debug1284_w5100
make debug1284_w5200
make debug1284_w5500

make debug2560_w5100
make debug2560_w5200
make debug2560_w5500

make debug328_w5100
make debug328_w5200
make debug328_w5500

cp *.hex ../../avr/bootloaders/ariadne/
